{
  "name": "@alexmchan/memocache",
  "version": "0.9.0",
  "private": false,
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.1",
    "@libsql/client": "^0.14.0",
    "just-memoize-last": "^3.2.0",
    "superjson": "^2.2.1",
    "@alexmchan/memocache-common": "workspace:^"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@repo/eslint-config": "workspace:^",
    "@alexmchan/memocache-store-redis": "workspace:^",
    "@testcontainers/redis": "^10.13.2",
    "ioredis": "^5.4.1",
    "testcontainers": "^10.13.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmchan/memocache"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsup",
    "check-exports": "yarn pack --out ./ignore-package.tgz && yarn attw ./ignore-package.tgz && rm ./ignore-package.tgz",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:watch": "vitest --watch"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
